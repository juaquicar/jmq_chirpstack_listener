[ req ]
default_bits       = 4096
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
CN = mosquitto_dev

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1   = mosquitto_dev
DNS.2   = mosquitto
DNS.3   = localhost
IP.1    = 127.0.0.1
